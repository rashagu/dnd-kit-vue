{
  "name": "@kousum/core",
  "version": "6.0.5",
  "description": "dnd kit – a lightweight React library for building performant and accessible drag and drop experiences",
  "author": "Claudéric Demers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kousum/dnd-kit-vue.git",
    "directory": "packages/core"
  },
  "scripts": {
    "dev": "vite",
    "rm:dist": "rimraf dist",
    "tsc": "tsc -b --force tsconfig.build.json",
    "build": "pnpm rm:dist && vue-tsc --noEmit && vite build && tsc -b --force tsconfig.build.json",
    "preview": "vite preview"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "dist"
  ],
  "dependencies": {
    "tslib": "^2.0.0",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.190",
    "@kousum/accessibility": "^3.0.0",
    "@kousum/utilities": "^3.2.0"
  },
  "peerDependencies": {
    "vue": "^3.2.45",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vue-tsc": "^1.0.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
